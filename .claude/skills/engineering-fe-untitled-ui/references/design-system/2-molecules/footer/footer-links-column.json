{
    "meta": {
        "name": "FooterLinksColumn",
        "description": "Column of footer links with a category heading",
        "category": "molecule",
        "figmaId": "3288:570969",
        "figmaFile": "7NsVWhvg7xLJUvGUyRkgEc",
        "version": "2.0.0"
    },

    "dependencies": {
        "atoms": [],
        "molecules": ["FooterLink"]
    },

    "variants": {
        "theme": ["default", "brand"]
    },

    "props": {
        "heading": {
            "type": "string",
            "required": true,
            "description": "Category heading for link group"
        },
        "links": {
            "type": "array",
            "required": true,
            "description": "Array of {label, href, badge?, external?}"
        }
    },

    "sizing": {
        "headingMarginBottom": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-xl" },
        "linkGap": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-lg" }
    },

    "tokens": {
        "default": {
            "heading": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-quaternary (500)" }
        },
        "brand": {
            "heading": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-quaternary_on-brand" }
        }
    },

    "textStyles": {
        "heading": {
            "typography": { "isAlias": true, "collection": "Typography Styles", "name": "Text sm/Semibold" }
        }
    },

    "accessibility": {
        "role": "navigation",
        "ariaAttributes": {
            "aria-labelledby": "heading id"
        }
    }
}

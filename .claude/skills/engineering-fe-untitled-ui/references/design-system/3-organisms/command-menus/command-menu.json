{
    "meta": {
        "name": "CommandMenu",
        "description": "Command palette for keyboard-driven navigation and actions",
        "category": "organism",
        "figmaId": "1247:133567",
        "figmaFile": "7NsVWhvg7xLJUvGUyRkgEc",
        "version": "2.0.0"
    },

    "dependencies": {
        "atoms": ["Icon", "Badge"],
        "molecules": ["InputField"]
    },

    "variants": {
        "size": ["sm", "md", "lg"]
    },

    "props": {
        "open": {
            "type": "boolean",
            "required": true
        },
        "size": {
            "type": "enum",
            "default": "md",
            "options": ["sm", "md", "lg"]
        },
        "placeholder": {
            "type": "string",
            "default": "Type a command or search..."
        },
        "groups": {
            "type": "array",
            "description": "Array of {label, items: [{id, label, icon?, shortcut?, onSelect}]}"
        },
        "recentItems": {
            "type": "array"
        },
        "emptyMessage": {
            "type": "string",
            "default": "No results found"
        },
        "onClose": {
            "type": "function"
        },
        "onSelect": {
            "type": "function"
        }
    },

    "sizing": {
        "sm": {
            "width": { "isAlias": true, "collection": "Width Styles", "name": "width-md" },
            "maxHeight": "400px"
        },
        "md": {
            "width": { "isAlias": true, "collection": "Width Styles", "name": "width-lg" },
            "maxHeight": "480px"
        },
        "lg": {
            "width": { "isAlias": true, "collection": "Width Styles", "name": "width-xl" },
            "maxHeight": "560px"
        },
        "headerPadding": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-xl" },
        "itemPadding": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-lg" },
        "groupGap": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-md" },
        "borderRadius": { "isAlias": true, "collection": "Radius Styles", "name": "radius-xl" },
        "iconSize": { "isAlias": true, "collection": "Sizing Styles", "name": "Icon/size-md" }
    },

    "tokens": {
        "overlay": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Background/bg-overlay" },
        "container": {
            "background": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Background/bg-primary" },
            "border": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Border/border-secondary" },
            "shadow": { "isAlias": true, "collection": "Effect Styles", "name": "Shadows/shadow-xl" }
        },
        "header": {
            "border": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Border/border-secondary" }
        },
        "item": {
            "default": {
                "background": null,
                "icon": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Foreground/fg-quaternary (500)" }
            },
            "hover": {
                "background": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Background/bg-secondary" }
            },
            "active": {
                "background": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Background/bg-active" },
                "icon": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Foreground/fg-brand-primary (600)" }
            }
        },
        "shortcut": {
            "background": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Background/bg-tertiary" },
            "border": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Border/border-secondary" }
        }
    },

    "textStyles": {
        "groupLabel": {
            "typography": { "isAlias": true, "collection": "Typography Styles", "name": "Text xs/Medium" },
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-quaternary (500)" }
        },
        "itemLabel": {
            "typography": { "isAlias": true, "collection": "Typography Styles", "name": "Text sm/Medium" },
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-secondary (700)" }
        },
        "shortcut": {
            "typography": { "isAlias": true, "collection": "Typography Styles", "name": "Text xs/Regular" },
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-quaternary (500)" }
        },
        "empty": {
            "typography": { "isAlias": true, "collection": "Typography Styles", "name": "Text sm/Regular" },
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-tertiary (600)" }
        }
    },

    "animation": {
        "enter": {
            "type": "scale-in",
            "duration": { "isAlias": true, "collection": "Motion Styles", "name": "duration-fast" }
        },
        "exit": {
            "type": "scale-out",
            "duration": { "isAlias": true, "collection": "Motion Styles", "name": "duration-fast" }
        }
    },

    "accessibility": {
        "role": "dialog",
        "ariaAttributes": {
            "aria-modal": "true",
            "aria-label": "Command menu"
        },
        "keyboardInteraction": {
            "Escape": "close menu",
            "ArrowUp": "previous item",
            "ArrowDown": "next item",
            "Enter": "select item",
            "Cmd+K": "open menu"
        },
        "notes": [
            "Focus should be trapped within menu",
            "Search results should be announced"
        ]
    }
}

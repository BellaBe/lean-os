{
    "meta": {
        "name": "SocialIcon",
        "description": "Social media brand icons with consistent sizing",
        "category": "shared-asset",
        "figmaId": "1457:244804",
        "figmaFile": "BFATWt2kajie3JJgagoDwr",
        "version": "2.0.0"
    },

    "variants": {
        "platform": ["x", "instagram", "apple", "github", "angellist", "tumblr", "facebook", "linkedin", "youtube", "tiktok", "dribbble", "behance", "pinterest", "threads"],
        "size": ["sm", "md", "lg"]
    },

    "props": {
        "platform": {
            "type": "enum",
            "required": true,
            "options": ["x", "instagram", "apple", "github", "angellist", "tumblr", "facebook", "linkedin", "youtube", "tiktok", "dribbble", "behance", "pinterest", "threads"]
        },
        "size": {
            "type": "enum",
            "default": "md",
            "options": ["sm", "md", "lg"]
        },
        "href": {
            "type": "string",
            "description": "Link URL for the social profile"
        }
    },

    "sizing": {
        "sm": {
            "size": { "isAlias": true, "collection": "Sizing Styles", "name": "Icon/size-md" }
        },
        "md": {
            "size": { "isAlias": true, "collection": "Sizing Styles", "name": "Icon/size-lg" }
        },
        "lg": {
            "size": { "isAlias": true, "collection": "Sizing Styles", "name": "Icon/size-xl" }
        }
    },

    "tokens": {
        "x": {
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Component colors/Components/Icons/Social icons/social-icon-fg-x" }
        },
        "instagram": {
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Component colors/Components/Icons/Social icons/social-icon-fg-instagram" }
        },
        "apple": {
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Component colors/Components/Icons/Social icons/social-icon-fg-apple" }
        },
        "github": {
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Component colors/Components/Icons/Social icons/social-icon-fg-github" }
        },
        "angellist": {
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Component colors/Components/Icons/Social icons/social-icon-fg-angellist" }
        },
        "tumblr": {
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Component colors/Components/Icons/Social icons/social-icon-fg-tumblr" }
        }
    },

    "accessibility": {
        "role": "link",
        "ariaLabel": "Visit our {platform} profile",
        "notes": "Always provide meaningful aria-label with platform name"
    }
}

{
    "meta": {
        "name": "UtilityIcons",
        "description": "Specialized utility icons for ratings, checks, and indicators",
        "category": "shared-asset",
        "figmaId": "1025:31781",
        "figmaFile": "BFATWt2kajie3JJgagoDwr",
        "version": "2.0.0"
    },

    "components": {
        "starIcon": {
            "figmaId": "1232:9",
            "description": "Star rating icon with fill states",
            "variants": {
                "state": ["empty", "half", "filled"],
                "size": ["xs", "sm", "md", "lg"]
            }
        },
        "checkIcon": {
            "figmaId": "1254:137887",
            "description": "Checkmark icon for success states and lists",
            "variants": {
                "style": ["simple", "circle", "square"],
                "size": ["xs", "sm", "md", "lg"]
            }
        },
        "checkItemText": {
            "figmaId": "1345:1610",
            "description": "Check item with text label for feature lists",
            "variants": {
                "size": ["sm", "md", "lg"],
                "checked": [true, false]
            }
        }
    },

    "sizing": {
        "starIcon": {
            "xs": { "size": { "value": 12, "unit": "px" } },
            "sm": { "size": { "value": 16, "unit": "px" } },
            "md": { "size": { "value": 20, "unit": "px" } },
            "lg": { "size": { "value": 24, "unit": "px" } }
        },
        "checkIcon": {
            "xs": { "size": { "isAlias": true, "collection": "Sizing Styles", "name": "Icon/size-xs" } },
            "sm": { "size": { "isAlias": true, "collection": "Sizing Styles", "name": "Icon/size-sm" } },
            "md": { "size": { "isAlias": true, "collection": "Sizing Styles", "name": "Icon/size-md" } },
            "lg": { "size": { "isAlias": true, "collection": "Sizing Styles", "name": "Icon/size-lg" } }
        }
    },

    "tokens": {
        "star": {
            "filled": { "isAlias": true, "collection": "Color Styles", "name": "Component colors/Utility/Warning/utility-warning-500" },
            "empty": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Border/border-secondary" }
        },
        "check": {
            "success": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Foreground/fg-success-primary" },
            "brand": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Foreground/fg-brand-primary (600)" },
            "default": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Foreground/fg-quaternary (500)" }
        },
        "checkItemText": {
            "icon": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Foreground/fg-success-primary" },
            "text": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-secondary (700)" },
            "gap": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-md" }
        }
    },

    "accessibility": {
        "starRating": {
            "role": "img",
            "ariaLabel": "{rating} out of 5 stars"
        },
        "checkIcon": {
            "role": "img",
            "ariaHidden": true
        }
    }
}

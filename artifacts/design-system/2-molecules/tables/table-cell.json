{
    "meta": {
        "name": "TableCell",
        "description": "Table cell with various content types (text, badge, avatar, actions)",
        "category": "molecule",
        "figmaId": "1222:107305",
        "figmaFile": "7NsVWhvg7xLJUvGUyRkgEc",
        "version": "2.0.0"
    },

    "dependencies": {
        "atoms": ["Badge", "Avatar", "Icon", "Checkbox", "Button"]
    },

    "variants": {
        "type": ["text", "text-secondary", "badge", "avatar", "checkbox", "actions", "link"],
        "alignment": ["left", "center", "right"]
    },

    "props": {
        "type": {
            "type": "enum",
            "default": "text",
            "options": ["text", "text-secondary", "badge", "avatar", "checkbox", "actions", "link"]
        },
        "value": {
            "type": "string"
        },
        "secondaryValue": {
            "type": "string"
        },
        "avatar": {
            "type": "object",
            "properties": {
                "src": { "type": "string" },
                "name": { "type": "string" }
            }
        },
        "badge": {
            "type": "object",
            "properties": {
                "label": { "type": "string" },
                "variant": { "type": "string" }
            }
        },
        "checked": {
            "type": "boolean"
        },
        "actions": {
            "type": "array"
        },
        "alignment": {
            "type": "enum",
            "default": "left",
            "options": ["left", "center", "right"]
        }
    },

    "sizing": {
        "paddingX": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-3xl" },
        "paddingY": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-xl" },
        "minHeight": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-8xl" },
        "avatarSize": { "isAlias": true, "collection": "Sizing Styles", "name": "Avatar/size-md" },
        "gap": { "isAlias": true, "collection": "Spacing Styles", "name": "spacing-lg" }
    },

    "tokens": {
        "border": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Border/border-secondary" }
    },

    "textStyles": {
        "primary": {
            "typography": { "isAlias": true, "collection": "Typography Styles", "name": "Text sm/Medium" },
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-primary (900)" }
        },
        "secondary": {
            "typography": { "isAlias": true, "collection": "Typography Styles", "name": "Text sm/Regular" },
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-tertiary (600)" }
        },
        "link": {
            "typography": { "isAlias": true, "collection": "Typography Styles", "name": "Text sm/Medium" },
            "color": { "isAlias": true, "collection": "Color Styles", "name": "Colors/Text/text-brand-secondary (700)" }
        }
    },

    "accessibility": {
        "role": "cell",
        "notes": [
            "Checkbox cells should have proper labels",
            "Action buttons should have accessible names"
        ]
    }
}
